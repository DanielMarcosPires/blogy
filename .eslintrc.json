{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "next/core-web-vitals",
        "@rocketseat/eslint-config/next"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "semi": true,
                "endOfLine": "auto"
            }
        ],
        "no-unused-vars": "error",
        "@typescript-eslint/no-explicit-any": "off"
    }
}